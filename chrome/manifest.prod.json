{
  "version": "0.5",
  "name": "Outgoing Requests Manager",
  "manifest_version": 2,
  "description": "This plugin lists out all strange request from your site as expected.",
  "icons": {
    "16": "img/16x16_flame.png",
    "48": "img/48x48_flame.png",
    "128": "img/128x128_flame.png"
  },
  "devtools_page": "devtools.html",
  "permissions": [
    "contextMenus",
    "tabs",
    "storage",
    "https://github.com/*"
  ],
  "content_security_policy": "default-src 'self'; script-src 'self'; style-src * 'unsafe-inline'; img-src 'self' data:;"
}
